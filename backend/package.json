    "eslint": "^8.40.0",
    "eslint-plugin-import": "^2.27.5",
    "nodemon": "^2.0.22",
    "prisma": "^4.14.1",
    "supertest": "^6.3.3",
    "ts-command-line-args": "^2.5.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.4",
    "vitest": "^0.31.1"
  },
