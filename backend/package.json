  "main": "index.js",
  "scripts": {
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "test:int": "yarn docker:up && cross-env NODE_ENV=test vitest run ; yarn docker:down",
    "test:intoof": "yarn docker:up && ts-node prisma/seed.ts && cross-env NODE_ENV=test vitest run ; yarn docker:down",
    "build": "tsc",
    "dev": "yarn docker:up && nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts ; yarn docker:down",
    "openapi": "npx openapi-client-axios-typegen typegen ../api/openapi.yaml > src/types/openapi.d.ts"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
